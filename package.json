{
  "name": "@stanislavpoleshuk/react-native-onboarding",
  "version": "0.0.4",
  "description": "A pure JavaScript <Onboarding /> component for react-native",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "contributors": [
    "Stanislav Poleshuk <stanislavpoleshuk@gmail.com> (https://github.com/stanislavpoleshuk)"
  ],
  "keywords": [
    "react-component",
    "react-native",
    "ios",
    "android"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./lib/* && yarn lint && tsc && yarn prettier",
    "prepare": "yarn build",
    "clean": "rimraf lib",
    "lint": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "prepublish": "yarn clean && yarn build",
    "postpublish": "git push && git push --tags",
    "prettier": "prettier --write \"{docs,example,src,lib}/**/*.{json,md,js,jsx,ts,tsx}\"",
    "version": "git add -A",
    "watch:onboarding": "yarn tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stanislavpoleshuk/react-native-onboarding.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.59.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/react-native": "^0.64.10",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "7.28.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "prettier": "^2.0.5",
    "react": "^17.0.2",
    "react-native": "^0.64.2",
    "react-native-typescript-transformer": "^1.2.13",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
